{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/_gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/_gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/_gateway",
  "projects": {
    "_gateway": {
      "type": "application",
      "root": "apps/_gateway",
      "entryFile": "main",
      "sourceRoot": "apps/_gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/_gateway/tsconfig.app.json"
      }
    }
  }
}